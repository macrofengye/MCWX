{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0",
    "slim/slim": "3.*",
    "doctrine/common": "*",
    "doctrine/orm": "*",
    "guzzlehttp/guzzle": "*",
    "doctrine/collections": "~1.2",
    "doctrine/dbal": "*",
    "doctrine/instantiator": "~1.0.1",
    "doctrine/cache": "~1.4",
    "monolog/monolog": "~1.18",
    "symfony/validator": "~3.0",
    "hassankhan/config": "*",
    "slim/flash": "~0.1",
    "slim/csrf": "~0.6",
    "beberlei/DoctrineExtensions": "*",
    "gedmo/doctrine-extensions": "*",
    "slim/twig-view": "*",
    "tuupola/cors-middleware": "*",
    "tuupola/slim-jwt-auth": "*",
    "vlucas/phpdotenv": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-exif": "*",
    "peppeocchi/php-cron-scheduler": "~1.0",
    "runcmf/runtracy": "~0.2"
  },
  "require-dev": {
    "stoneworld/wechat": "dev-master",
    "overtrue/wechat": "~3.1",
    "sodabinbin/enterprise-wechat": "dev-master",
    "symfony/yaml": "2.5.*@dev",
    "satooshi/php-coveralls": "dev-master",
    "phpunit/phpunit": "~4.0",
    "phpunit/dbunit": "1.*",
    "facebook/webdriver": "~1.0",
    "mysql-workbench-schema-exporter/mysql-workbench-schema-exporter": "^3.0.0",
    "mysql-workbench-schema-exporter/doctrine2-exporter": "^3.0.0"
  },
  "bin": [
    "composer/bin/dbunit"
  ],
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Polymer\\": "framework\\",
      "MComponent\\": "component\\",
      "Entity\\": "entity\\",
      "Task\\": "task\\",
      "WeChat\\": "app\\WeChat\\"
    },
    "files": [
      "framework\\Lib\\functions_include.php"
    ]
  }
}
